    "style-loader": "0.13.1",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-validator": "2.2.7"
  },
  "config": {
    "ghooks": {
      "pre-commit": "opt --in pre-commit --exec \"npm run validate\""
    }
  },
  "scripts": {
    "test": "./node_modules/karma/bin/karma start karma.conf.js --single-run",
    "watch:test": "karma start --auto-watch --no-single-run",
    "validate": "npm-run-all --parallel validate-webpack:* lint test build",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "clean-dist": "rimraf dist",
    "copy-files": "cpy 'src/favicon.ico' 'src/apple-touch-icon.png' dist && cpy 'src/images/**/*' dist/images && cpy src/fonts/**/* dist/fonts",
    "clean-and-copy": "npm run clean-dist && npm run copy-files",
    "prestart": "npm run clean-and-copy",
    "start": "webpack-dev-server --hot --inline --env.dev --content-base dist",
    "prebuild": "npm run clean-and-copy",
    "prebuild:prod": "npm run clean-and-copy",
    "build": "webpack --env.dev",
    "build:prod": "webpack --env.prod -p",
    "lint": "eslint ."
  }
}
